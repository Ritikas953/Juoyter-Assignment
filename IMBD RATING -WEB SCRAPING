    "for li in div.find_all('li'):\n",
    "    name = li.find('h3',class_='ipc-title__text').text\n",
    "    rating = li.find('span',class_='ipc-rating-star').text\n",
    "    year = li.find('span',class_='sc-b189961a-8 kLaxqf dli-title-metadata-item').text\n",
    "    # df = pd.DataFrame({'Name':data})\n",
    "    res1 = \" \".join(re.findall(\"[a-zA-Z]+\", name))\n",
    "    print(res1)\n",
    "    print(rating)\n",
    "    print(year)"
